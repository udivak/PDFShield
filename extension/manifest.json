{
    "manifest_version": 3,
    "name": "PDFShield - PII Redactor",
    "version": "1.0",
    "description": "Upload a PDF to redact PII automatically using a local Python server.",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}